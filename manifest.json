{
  "manifestVersion": 1,
  "id": "blitz-allbranches",
  "version": "0.5.13",
  "name": "Branch Blitz",
  "description": "Shows all non-main branches across all repositories in your Project.",
  "publisher": "sierpinski",
  "icons": {
    "default": "images/blitzallbranches.png"
  },
  "categories": [
    "Code"
  ],
  "tags": [
    "branches",
    "git"
  ],
  "galleryFlags": [
    "Public"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license-terms.md"
    }
  },
  "screenshots": [
    {
      "path": "images/ScreenCaptureExtv03.png"
    },
    {
      "path": "images/ScreenCapWidget.png"
    }
  ],
  "links": {
    "home": {
      "uri": "https://github.com/sierpinski/BlitzBranches"
    },
    "support": {
      "url": "https://github.com/sierpinski/BlitzBranches"
    },
    "privacypolicy": {
      "url": "https://creativecommons.org/privacy/"
    }
  },
  "branding": {
    "color": "rgb(90, 86, 206)",
    "theme": "dark"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "BlitzActiveBranches",
      "type": "ms.vss-web.hub",
      "description": "Shows all non-main branches across all repositories in your Project.",
      "targets": [
        "ms.vss-code-web.code-hub-group"
      ],
      "properties": {
        "name": "Non-Main Branches",
        "order": 99,
        "uri": "blitzallbranches.html",
        "icon": "images/babmenuicon.png"
      }
    },
    {
      "id": "BlitzActiveBranchesWidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog"
      ],
      "properties": {
        "name": "Non-Main Branch Count",
        "description": "Count of all non-main branches across all of your project's repositories.",
        "catalogIconUrl:": "images/CatalogIcon.png",
        "previewImageUrl": "images/preview.png",
        "uri": "blitzactivebrancheswidget.html",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 1
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    }
  ],
  "scopes": [
    "vso.code"
  ],
  "files": [
    {
      "path": "blitzallbranches.html",
      "addressable": true
    },
    {
      "path": "blitzactivebrancheswidget.html",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "styles",
      "addressable": true
    },
    {
      "path": "sdk/scripts",
      "addressable": true
    },
    {
      "path": "images/CatalogIcon.png",
      "addressable": true
    },
    {
      "path": "images/preview.png",
      "addressable": true
    },
    {
      "path": "images/babmenuicon.png",
      "addressable": true
    }
  ]
}